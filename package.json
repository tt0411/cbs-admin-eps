{
  "name": "@sa/elp",
  "type": "module",
  "version": "0.0.1",
  "description": "一个基于Vue3、Vite3、TypeScript、ElementPlus and UnoCSS的清新优雅的中后台模版。(封装table，函数式调用dialog，drawer, 动态详情页面)",
  "license": "MIT",
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite5",
    "TypeScript",
    "element-plus",
    "element-plus-admin",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "eslint . --fix",
    "postbuild": "sa print-cbs",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@antv/data-set": "0.11.8",
    "@antv/g2": "5.2.11",
    "@antv/g6": "5.0.44",
    "@better-scroll/core": "2.5.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@iconify/vue": "4.3.0",
    "@sa/alova": "workspace:*",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@visactor/vchart": "1.13.6",
    "@visactor/vchart-theme": "1.12.2",
    "@visactor/vtable-editors": "1.17.0",
    "@visactor/vtable-gantt": "1.17.0",
    "@visactor/vue-vtable": "1.17.0",
    "@vueuse/components": "12.8.2",
    "@vueuse/core": "12.8.2",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "defu": "^6.1.4",
    "dhtmlx-gantt": "9.0.5",
    "dompurify": "3.2.4",
    "echarts": "5.6.0",
    "element-plus": "^2.9.5",
    "hash.js": "^1.1.7",
