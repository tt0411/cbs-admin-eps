    "swiper": "11.2.5",
    "tailwind-merge": "3.0.2",
    "typeit": "8.8.7",
    "vditor": "3.10.9",
    "vue": "3.5.13",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.1.1",
    "vue-pdf-embed": "2.1.2",
